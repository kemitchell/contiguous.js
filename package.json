{
  "name": "contiguous",
  "description": "check an Array for contiguous elements matching a predicate",
  "version": "1.0.1",
  "author": {
    "name": "Kyle E. Mitchell",
    "email": "kyle@kemitchell.com",
    "url": "http://kemitchell.com/"
  },
  "bugs": "https://github.com/kemitchell/contiguous.js/issues",
  "devDependencies": {
    "check-versions": "^1.0.0",
    "jscs": "^1.12.0",
    "jshint": "^2.7.0",
    "literate-assertions": "^0.1.1"
  },
  "homepage": "https://github.com/kemitchell/contiguous.js",
  "keywords": [
    "arrays",
    "predicates",
    "validation"
  ],
  "license": "Apache-2.0",
  "main": "contiguous.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kemitchell/contiguous.js.git"
  },
  "scripts": {
    "check": "check-versions --check-module",
    "lint": "jshint contiguous.js && jscs contiguous.js",
    "pre-commit": "npm run lint && npm run check && npm run test",
    "test": "literate-assertions < README.md | sed \"s!'contiguous'!'./'!g\" | node",
    "travis-ci": "npm run pre-commit"
  }
}
